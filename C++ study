//2022. 01. 10
2강 Hello, World! 해석하기

#include <stdio.h>

//<stdio.h>라는 library를 include해라.
int main() {
//main function을 실행
  printf("Hello, World!\n);
// \n 줄바꿈 command
}
-------------------------------------------
3강 printf로 다양한 데이터 출력하기

#include <stdio.h>

int main() {
  // 2 + 3 = 5
  printf("%d + %d = %d\n", 2, 3, 5);
  // 3.140000
  printf("%f\n", 3.14);
  // 3.14
  printf("%.2f\n", 3.141592);
  
  // 3.14159
  printf("%g\n", 3.141592);
  //8.72e+09
  printf("%.3g\n", 8723984712.12837468723);
  
  // a b c
  printf("%c %c %c\n", 'a', 'b','c');
  
  // 안녕하세요
  printf(%s\n", "안녕하세요");
}
